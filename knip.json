{
	"$schema": "https://unpkg.com/knip@latest/schema.json",
	"entry": ["src/index.ts!"],
	"ignoreExportsUsedInFile": {
		"interface": true,
		"type": true
	},
	"project": ["src/**/*.ts!"],
	"workspaces": {
		"packages/*": {
			"entry": "{index}.ts",
			"project": "**/*.ts"
		},
		"examples/*": {
			"entry": "{index}.ts",
			"project": "**/*.ts"
		}
	}
}
